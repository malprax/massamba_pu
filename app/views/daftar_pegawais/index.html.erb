<h1>Listing daftar_pegawais</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Nip</th>
      <th>Pangkat</th>
      <th>Golongan pangkat</th>
      <th>Jabatan</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @daftar_pegawais.each do |daftar_pegawai| %>
      <tr>
        <td><%= daftar_pegawai.name %></td>
        <td><%= daftar_pegawai.nip %></td>
        <td><%= daftar_pegawai.pangkat_id %></td>
        <td><%= daftar_pegawai.golongan_pangkat %></td>
        <td><%= daftar_pegawai.jabatan_id %></td>
        <td><%= link_to 'Show', daftar_pegawai %></td>
        <td><%= link_to 'Edit', edit_daftar_pegawai_path(daftar_pegawai) %></td>
        <td><%= link_to 'Destroy', daftar_pegawai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daftar pegawai', new_daftar_pegawai_path %>
