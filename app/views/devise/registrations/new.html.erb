<div class="wrap">
  <div class="container">
    <div class="row">
      <div class="span4"></div>
      <div class="span4">
        <h3>Daftar User baru</h3>

        <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {honeypot: true}) do |f| %>
          <%= devise_error_messages! %>

          <div><%= f.label :email %><br />
          <%= f.email_field :email, autofocus: true %></div>

          <div><%= f.label :password %><br />
            <%= f.password_field :password, autocomplete: "off" %></div>

          <div><%= f.label :password_confirmation %><br />
            <%= f.password_field :password_confirmation, autocomplete: "off" %></div>

          <div><%= f.submit "Daftar", class: 'btn btn-primary' %></div>
        <% end %>

        <p><%= render "devise/shared/links" %></p>
        <p><%= link_to "Kembali ke Beranda Â»", root_url %></p>
      </div>
      <div class="span4"></div>
    </div>
  </div>
</div>

    
 
