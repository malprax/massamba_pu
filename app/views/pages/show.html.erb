   
      <!-- Begin page content -->
      <div class="wrap">
        <%= render "shared/navigation" %>
        <div class="container">
          <div class="page-header">
            <h1><%= @page.name %></h1>
          </div>
          <p class="lead"><%= @page.content %></p>          
